		{
			"name": "Create Valid User",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Response to have specified elements: \", () => {\r",
							"    const body = pm.response.json();\r",
							"    pm.expect(body).to.have.ownProperty(\"accessToken\");\r",
							"    pm.expect(body).to.have.ownProperty(\"id\");\r",
							"})\r",
							"\r",
							"pm.globals.set(\"valid_jwt\", pm.response.json().accessToken);\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\r\n    \"firstname\":\"{{$randomFirstName}}\",\r\n    \"lastname\":\"{{$randomLastName}}\",\r\n    \"email\":\"{{$randomEmail}}\",\r\n    \"password\":\"BrandonRagghianti992!\",\r\n    \"username\":\"{{$randomUserName}}\",\r\n    \"role\":\"1\",\r\n    \"phone\":\"{{$randomPhoneNumber}}\"\r\n}",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": "localhost:4000/register"
			},
			"response": []
		},
